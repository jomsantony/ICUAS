<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.icudirect"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.icudirect.ScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.icudirect.MainActivity"
            android:label="Patient Demographics" >
        </activity>
        <activity
            android:name="com.example.icudirect.PatientDataActivity"
            android:label="@string/title_activity_patient_data" >
        </activity>
        <activity
            android:name="com.example.icudirect.LabResultsActivity"
            android:label="@string/title_activity_lab_results" >
        </activity>
        <activity
            android:name="com.example.icudirect.ChartsActivity"
            android:label="@string/title_activity_charts" >
        </activity>
        <activity
            android:name="com.example.icudirect.ImagesActivity"
            android:label="@string/title_activity_images" >
        </activity>
        <activity
            android:name="com.example.icudirect.ProgressNotesActivity"
            android:label="@string/title_activity_progress_notes" >
        </activity>
        <activity
            android:name="com.example.icudirect.ClinicalOrdersActivity"
            android:label="@string/title_activity_clinical_orders" >
        </activity>
    </application>

</manifest>